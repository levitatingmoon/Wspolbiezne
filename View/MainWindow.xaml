<Window x:Class="View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <TextBox x:Name="NumerTextBox" Width="300" Height="50" PreviewTextInput="PreviewTextInput"></TextBox>

        <Button x:Name="ButtonStart" Width="100" Height="50" Click="Button_Click">Start</Button>
        <Canvas>
            <Rectangle Width="800" Height="300" Fill="White" Stroke="Black" Canvas.Top="20" StrokeThickness="1" />
        </Canvas>
    </StackPanel>
</Window>
